<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="engsoft.profilerproject.cadocorrencia">

    <ImageView
        android:id="@+id/imageView16"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@android:color/background_light" />

    <ImageView
        android:id="@+id/imageView18"
        android:layout_width="0dp"
        android:layout_height="59dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@color/colorPrimary" />

    <ImageView
        android:id="@+id/imageView19"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@mipmap/logoprofiler"
        app:layout_constraintTop_toTopOf="@+id/imageView18"
        android:layout_marginTop="8dp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView18"
        android:layout_marginBottom="8dp"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="@+id/imageView18"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="8dp" />

    <ImageView
        android:id="@+id/imageView20"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@android:drawable/btn_dialog"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="8dp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView18"
        android:layout_marginBottom="8dp"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginStart="8dp" />

    <ImageView
        android:id="@+id/imageView21"
        android:layout_width="161dp"
        android:layout_height="118dp"
        app:srcCompat="@mipmap/photoprofiler"
        app:layout_constraintTop_toBottomOf="@+id/imageView19"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginTop="5dp"
        app:layout_constraintHorizontal_bias="0.502" />

    <ImageView
        android:id="@+id/imageView22"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@mipmap/uploadprofiler"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintHorizontal_bias="0.756"
        app:layout_constraintTop_toTopOf="@+id/imageView21"
        android:layout_marginTop="8dp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView21"
        android:layout_marginBottom="8dp"
        app:layout_constraintVertical_bias="1.0"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="8dp" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="80dp"
        android:layout_height="36dp"
        android:layout_marginLeft="0dp"
        android:text="Nome :"
        android:textColor="@android:color/background_dark"
        android:textSize="24sp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView21"
        android:layout_marginStart="8dp"
        android:layout_marginTop="-18dp" />

    <EditText
        android:id="@+id/editText2"
        android:layout_width="268dp"
        android:layout_height="37dp"
        android:layout_marginBottom="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:ems="10"
        android:inputType="textPersonName"
        app:layout_constraintBottom_toTopOf="@+id/textView10"
        app:layout_constraintLeft_toRightOf="@+id/textView9"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView21"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="8dp"
        app:layout_constraintHorizontal_bias="0.333"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="148dp"
        android:layout_height="37dp"
        android:layout_marginLeft="16dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="0dp"
        android:text="Localidade :"
        android:textAllCaps="false"
        android:textColor="@android:color/background_dark"
        android:textSize="24sp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView9" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="148dp"
        android:layout_height="37dp"
        android:layout_marginLeft="8dp"
        android:text="Data/Hora :"
        android:textColor="@android:color/background_dark"
        android:textSize="24sp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView10"
        android:layout_marginStart="8dp"
        android:layout_marginTop="34dp" />

    <TextView
        android:id="@+id/textView12"
        android:layout_width="106dp"
        android:layout_height="36dp"
        android:layout_marginLeft="8dp"
        android:text="Status  :"
        android:textColor="@android:color/background_dark"
        android:textSize="24sp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView13"
        android:layout_marginStart="8dp"
        android:layout_marginTop="0dp" />

    <TextView
        android:id="@+id/textView13"
        android:layout_width="106dp"
        android:layout_height="36dp"
        android:layout_marginLeft="0dp"
        android:text="Tipo :"
        android:textColor="@android:color/background_dark"
        android:textSize="24sp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView11"
        android:layout_marginStart="8dp"
        android:layout_marginTop="11dp" />

    <TextView
        android:id="@+id/textView18"
        android:layout_width="144dp"
        android:layout_height="34dp"
        android:text="Descrição  :"
        android:textColor="@android:color/background_dark"
        android:textSize="24sp"
        app:layout_constraintTop_toBottomOf="@+id/textView12"
        android:layout_marginLeft="0dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginStart="8dp"
        android:layout_marginTop="-23dp" />

    <EditText
        android:id="@+id/editText6"
        android:layout_width="114dp"
        android:layout_height="39dp"
        android:ems="10"
        android:inputType="date"
        app:layout_constraintLeft_toRightOf="@+id/textView11"
        android:layout_marginLeft="-144dp"
        app:layout_constraintBottom_toBottomOf="@+id/textView11"
        app:layout_constraintTop_toTopOf="@+id/textView11"
        android:layout_marginStart="8dp"
        app:layout_constraintVertical_bias="0.0" />

    <EditText
        android:id="@+id/editText7"
        android:layout_width="85dp"
        android:layout_height="39dp"
        android:ems="10"
        android:inputType="time"
        android:layout_marginBottom="8dp"
        app:layout_constraintTop_toTopOf="@+id/editText6"
        app:layout_constraintBottom_toBottomOf="@+id/editText6"
        app:layout_constraintLeft_toRightOf="@+id/editText6"
        android:layout_marginLeft="7dp"
        app:layout_constraintVertical_bias="0.0"
        android:layout_marginStart="8dp" />

    <Spinner
        android:id="@+id/spinner2"
        android:layout_width="241dp"
        android:layout_height="35dp"
        android:entries="@array/Tipos"
        app:layout_constraintBottom_toBottomOf="@+id/textView12"
        android:layout_marginBottom="8dp"
        android:layout_marginTop="0dp"
        app:layout_constraintTop_toBottomOf="@+id/spinner3"
        app:layout_constraintVertical_bias="0.222"
        app:layout_constraintLeft_toRightOf="@+id/textView12"
        android:layout_marginLeft="-26dp" />

    <Spinner
        android:id="@+id/spinner3"
        android:layout_width="241dp"
        android:layout_height="39dp"
        android:entries="@array/Tipos"
        android:layout_marginBottom="0dp"
        android:layout_marginTop="8dp"
        app:layout_constraintBottom_toBottomOf="@+id/textView13"
        app:layout_constraintTop_toBottomOf="@+id/editText6"
        app:layout_constraintLeft_toRightOf="@+id/textView13"
        android:layout_marginLeft="-14dp"
        app:layout_constraintVertical_bias="1.0" />

    <EditText
        android:id="@+id/editText8"
        android:layout_width="357dp"
        android:layout_height="71dp"
        android:ems="10"
        android:inputType="textMultiLine"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginTop="-48dp"
        app:layout_constraintTop_toBottomOf="@+id/textView18"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintHorizontal_bias="0.0" />

    <Button
        android:id="@+id/button4"
        android:layout_width="389dp"
        android:layout_height="46dp"
        android:layout_marginBottom="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginTop="8dp"
        android:background="@color/colorPrimary"
        android:text="Salvar"
        android:textColor="@android:color/background_light"
        android:textSize="30sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.476"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editText8"
        app:layout_constraintVertical_bias="0.0" />

    <EditText
        android:id="@+id/editText19"
        android:layout_width="365dp"
        android:layout_height="41dp"
        android:ems="10"
        android:inputType="text"
        android:hint="Rua, Bairro, Cidade"
        app:layout_constraintBottom_toTopOf="@+id/textView11"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginLeft="0dp"
        android:layout_marginBottom="-378dp" />

    <ImageView
        android:id="@+id/imageView38"
        android:layout_width="37dp"
        android:layout_height="29dp"
        android:visibility="invisible"
        app:srcCompat="@mipmap/right"
        app:layout_constraintBottom_toTopOf="@+id/editText19"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginBottom="1dp"
        android:layout_marginRight="191dp" />

    <ImageView
        android:id="@+id/imageView39"
        android:layout_width="37dp"
        android:layout_height="31dp"
        android:visibility="invisible"
        app:srcCompat="@mipmap/wrong"
        app:layout_constraintBottom_toTopOf="@+id/editText19"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginRight="191dp"
        android:layout_marginBottom="-1dp" />

</android.support.constraint.ConstraintLayout>
