<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".listocorrencia">


    <ImageView
        android:id="@+id/imageView34"
        android:layout_width="65dp"
        android:layout_height="59dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true"
        app:srcCompat="@color/colorPrimary"
        android:layout_alignParentEnd="true" />


    <ImageView
        android:id="@+id/imageView33"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:srcCompat="@android:color/background_light"
        android:layout_toEndOf="@+id/imageView34" />



    <ImageView
        android:id="@+id/imageView35"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@mipmap/logoprofiler"
        android:layout_marginRight="8dp"
        android:layout_marginLeft="8dp"
        android:layout_centerHorizontal="true" />

    <ImageView
        android:id="@+id/imageView36"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="8dp"
        app:srcCompat="@android:drawable/btn_dialog"
        android:layout_alignBottom="@+id/imageView35"
        android:layout_alignParentStart="true"
        android:layout_marginStart="13dp" />

    <EditText
        android:id="@+id/editText9"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:inputType="textPersonName"
        android:text="Pesquisar"
        android:layout_above="@+id/listFragment"
        android:layout_centerHorizontal="true" />

    <fragment
        android:layout_width="match_parent"
        android:layout_height="450dp"
        android:name="engsoft.profilerproject.listOcorrenciaFragment"
        android:id="@+id/listFragment"
        android:layout_alignParentBottom="true"
        android:layout_alignEnd="@+id/imageView33" />

</RelativeLayout>
